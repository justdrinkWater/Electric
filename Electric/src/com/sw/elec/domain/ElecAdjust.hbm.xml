<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.sw.elec.domain.ElecAdjust" table="Elec_Adjust">
		<id name="seqID" type="integer">
			<column name="seqID" not-null="true"></column>
			<generator class="increment"></generator>
		</id>
		<property name="devID" type="string">
			<column name="devID"></column>
		</property>
		<property name="isAdjust" type="string">
			<column name="isAdjust"></column>
		</property>
		<property name="adjustDate" type="date">
			<column name="adjustDate"></column>
		</property>
		<property name="overhaulDate" type="date">
			<column name="overhaulDate"></column>
		</property>
		<property name="isHaving" type="string">
			<column name="isHaving"></column>
		</property>
		<property name="record" type="string">
			<column name="record"></column>
		</property>
		<property name="comment" type="string">
			<column name="comment"></column>
		</property>
		<property name="isDelete" type="string">
			<column name="isDelete"></column>
		</property>
		<property name="createEmpID" type="string">
			<column name="createEmpID"></column>
		</property>
		<property name="createDate" type="date">
			<column name="createDate"></column>
		</property>
		<property name="lastEmpID" type="string">
			<column name="lastEmpID"></column>
		</property>
		<property name="lastDate" type="date">
			<column name="lastDate"></column>
		</property>
	</class>
</hibernate-mapping>